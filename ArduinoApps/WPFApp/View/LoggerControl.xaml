<UserControl x:Class="WPFApp.View.LoggerControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WPFApp.View"
             xmlns:vm="clr-namespace:WPFApp.ViewModel"
             mc:Ignorable="d" 
             >
    
    <UserControl.DataContext>
        <vm:LoggerControlViewModel/>
    </UserControl.DataContext>
    <Grid>
        <Label Content="データログ" Margin="5"/>
        <ListView ItemsSource="{Binding ListView}" SelectedItem="{Binding SelectItem.Value}"/>
    </Grid>
</UserControl>
